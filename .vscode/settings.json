{
  "files.exclude": {
    "**/node_modules": true,
    "dist/**": true,
    "worker/**": true,
    "static/worker.js": true
  },
  "git.ignoreLimitWarning": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "dist/**": true
  },
  "files.watcherExclude": {
    "dist/**": true
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2
}
